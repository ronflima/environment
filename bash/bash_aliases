# -*-shell-script-*-

# Calls emacs under the hood
function e() {
    emacs $* &
}

# Calls django manager, if available
function mn(){
    if [ -f manage.py ]
    then
        python manage.py $*
    fi
}

alias chicle='cd /mnt/d/projetos/chicleteMkt/src'
alias myself='cd /mnt/d/projetos/pessoais'
